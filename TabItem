// Write your code here
const TabItem = props => {
  const {item, clickItem} = props

  const onClickPara = () => {
    clickItem(item.tabId)
  }

  return (
    <li>
      <button type="button" onClick={onClickPara}>
        {item.displayText}
      </button>
    </li>
  )
}
export default TabItem
